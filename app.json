{
	"name": "cloud-gox",
	"website": "https://github.com/jpillora/cloud-gox",
	"repository": "https://github.com/jpillora/cloud-gox",
	"env": {
		"BUILDPACK_URL": "http://github.com/jpillora/heroku-buildpack-go.git",
		"HTTP_USER": {
			"description": "HTTP authentication user",
			"required": false
		},
		"HTTP_PASS": {
			"description": "HTTP authentication password",
			"required": false
		},
		"GH_USER": {
			"description": "Github user (requires password)",
			"required": false
		},
		"GH_PASS": {
			"description": "Github password (requires user)",
			"required": false
		},
		"GH_TOKEN": {
			"description": "Github API token",
			"required": false
		}
	}
}